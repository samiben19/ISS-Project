<hibernate-mapping package = "domain">
    <class name="Film" table="Filme">
        <id name="id" type="long" column="id">
            <generator class="identity" />
        </id>
        <property name="nume" type="string">
            <column name="nume" />
        </property>
        <property name="autor" type="string">
            <column name="autor"/>
        </property>
    </class>
</hibernate-mapping>